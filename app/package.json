{
  "name": "@opentrons/ot-app",
  "version": "2.4.2",
  "description": "Opentrons desktop application",
  "main": "shell/main.js",
  "scripts": {
    "test": "make test",
    "start": "make dev",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Opentrons/opentrons.git"
  },
  "author": {
    "name": "Opentrons Labworks",
    "email": "engineering@opentrons.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Opentrons/opentrons/issues"
  },
  "homepage": "https://github.com/Opentrons/opentrons",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|ttf|woff|woff2)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!internals/**",
      "!**/dist/**",
      "!webpack**",
      "!coverage/**",
      "!**/test/**"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/"
    ],
    "env": [
      "node",
      "browser",
      "jest"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-css-modules"
    ],
    "ignoreFiles": [
      "**/dist/**",
      "**/coverage/**"
    ]
  },
