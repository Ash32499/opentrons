{
  "env": {
    "test": {
      "plugins":
        ["istanbul"]
    },
    "production": {
      "presets": ["react-optimize"],
      "plugins": ["babel-plugin-dev-expression"]
    },
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes",
        "react-hot-loader/babel"
      ]
    }
  },
  "plugins": ["add-module-exports", "dynamic-import-webpack"],
  "presets": [
    [
      "env", {
        "targets": {"node": 6},
        "useBuiltIns": true
      }
    ],
    "stage-2",
    "react",
    "es2015",
    "es2017"
  ]
}
