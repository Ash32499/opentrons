{
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "presets": [
    "react",
    [
      "env", {
        "targets": {
          "node": "6.11",
          "electron": "1.6"
        },
        "modules": false
      }
    ]
  ]
}
