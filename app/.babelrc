{
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "presets": [
        "react-optimize"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  },
  "plugins": [
    "babel-plugin-transform-object-rest-spread"
  ],
  "presets": [
    "react",
    [
      "env", {
        "targets": {
          "node": "6.11",
          "electron": "1.6"
        },
        "useBuiltIns": true
      }
    ]
  ]
}
